#include <bits/stdc++.h>

using namespace std;

int main()
{
    int t,i,n,j,q,b,res,flag;
    cin>>q;
    for(i=0;i<q;i++)
    {
        cin>>n;
        if(n%2==1)
        {
            cout<<'A'<<endl;
        }
        else
        {
           res=0;
           while(n%2==0)
           {
               n=n/2;
               res++;
           }
           if(res%2==0)
           {
               cout<<'A'<<endl;
           }
           else
           {
               cout<<'B'<<endl;
           }

        }
    }
    return 0;
}
