#include <bits/stdc++.h>

using namespace std;
long long jiecheng(long long n)
{
    if(n==1)
    {
        return 1;
    }
    else{
        return n*jiecheng(n-1);
    }
}
int main()
{
    long long a;
    int t,i,j;
    cin>>t;
    for(i=0;i<t;i++)
    {
        cin>>a;
        cout<<a-1<<endl;
    }
//    for(i=2;i<25;i++)
//    {
//        long long sum=0;
//        for(j=1;j<=i-1;j++)
//        {
//          sum+=jiecheng(j)*j;
//          sum%=i;
//        }
//        cout<<sum<<endl;
//    }
    return 0;
}
